namespace SmartSoft.Domain.Common
{
	using System;
	
	/// <summary>
	/// sysRoleLayoutView 
	/// </summary>
	[Serializable]
	public class sysRoleLayoutView
	{
		public sysRoleLayoutView()
		{
			
		}
		
		private int _rlvid;
		private int _objectid;
		private int _layoutorviewid;
		private int _controlid;
		private int? _actionid;
		
		///<sumary>
		/// ID
		///</sumary>
		public int rlvID
		{
			get{return _rlvid;}
			set{_rlvid = value;}
		}
		///<sumary>
		/// 对象ID
		///</sumary>
		public int ObjectID
		{
			get{return _objectid;}
			set{_objectid = value;}
		}
		///<sumary>
		/// 布局或视图ID
		///</sumary>
		public int LayoutOrViewID
		{
			get{return _layoutorviewid;}
			set{_layoutorviewid = value;}
		}
		///<sumary>
		/// 控件ID
		///</sumary>
		public int ControlID
		{
			get{return _controlid;}
			set{_controlid = value;}
		}
		///<sumary>
		/// 动作
		///</sumary>
		public int? ActionID
		{
			get{return _actionid;}
			set{_actionid = value;}
		}
	}
}
