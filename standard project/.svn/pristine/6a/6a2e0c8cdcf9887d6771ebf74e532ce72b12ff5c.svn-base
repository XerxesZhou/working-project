    white-space: nowrap;
}

.integral-rule-table tbody tr:hover {
    background: #f2f2f2;
}

.integral-table-content .integral-rule-table {
    border-top: none;
    border-bottom: none;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
}

.integral-rule-left .integral-table-content,
.integral-rule-right .integral-table-content {
    overflow-y: auto;
    height: -moz-calc(100% - 125px);
    height: -webkit-calc(100% - 125px);
