    line-height: 32px;
    height: 32px;
    padding: 0 10px;
}

.integral-rule-body {
    overflow: hidden;
    width: -webkit-calc(100% - 30px);
    width: -moz-calc(100% - 30px);
    width: calc(100% -30px);
    height: -webkit-calc(100% - 22px);
    height: -moz-calc(100% - 22px);
    height: calc(100% - 22px);
    margin-top: 25px;
}

.integral-rule-left {
    float: left;
    width: 30%;
    height: 100%;
    margin-right: 30px;
}

.integral-rule-right {
    float: left;
    width: 62%;
    height: 100%;
}

.integral-rule-table {
    width: 100%;
    border: 1px solid #dfdfdf;
    border-radius: 3px;
}

