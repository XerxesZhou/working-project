/*logo设置*/
.logo-upload-header span.title{
    display: block;
    color: #334455;
    font-size: 15px;
    margin-bottom: 10px;
}
.logo-upload-header span.des{
    color: #667b8f;
    display: block;
    margin-bottom: 20px;
}
.logo-upload-content{
    margin-bottom: 40px;
    overflow: hidden;
}
.logo-upload-content .upload-btn{
    border:1px solid #d6e1ee;
    border-radius: 3px;
    padding: 5px;
    display: inline-block;
    background: #fff;
    float: left;
}
.logo-upload-content .upload-button-text{
    width: 140px;
    height: 50px;
    line-height: 50px;
    background: #f7f9fc;
    color: #667b8f;
    display: inline-block;
    text-align: center;
}
.logo-upload-content .upload-button-text:hover{
    text-decoration: none;
    color: #00aaef;
    background: #f2f6fa;
}
.reupload-btn{
    float: left;
    margin-left: 20px;
    cursor: pointer;
}
.reupload-btn:hover{
    text-decoration: underline;
}
.logo-upload-content > a{
  float: left;
  font-size: 12px;
  vertical-align: baseline;
  margin-top: 0px;
  margin-left: 20px;
}
.logo-upload-des{
    clear: both;
}
.logo-upload-des p{
    color: #667b8f;
    line-height: 26px;
}
.logo-upload-des p:nth-child(3){
    margin-bottom: 20px;
}
.upload-demo-logo{
   border:1px solid #d6e1ee;
   border-radius: 3px;
   display: inline-block;
   padding: 5px;
   background: #fff;
   height: 53px;
   line-height: 53px;
   margin-top: 10px;
}
.upload-demo-logo img{
    width: 120px;
    height: 40px;
}

