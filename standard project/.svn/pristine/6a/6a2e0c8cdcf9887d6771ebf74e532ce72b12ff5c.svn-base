/*nav-menu-up*/

.nav-menu-up {
    display: none;
    width: 60px;
}

.nav-menu-up .sidebar-toggle {
    display: inline-block;
    width: 20px;
    height: 20px;
    text-align: center;
}

.nav-menu-up .sidebar-toggle span {
    display: inline-block;
    /*padding: 20px 20px 0 0;*/
    background: url(/@images/left_nav_ico.png) -2px -16px no-repeat;
}
.nav-menu-up .sidebar-toggle:hover span{
  background: url(/@images/left_nav_ico_hover.png) -2px -16px no-repeat;
}
.nav-menu-up .page-link {
    height: 40px;
    margin-bottom: 10px;
}

.nav-menu-up .page-menu .page-link {
    padding: 0;
}

.nav-menu-up .page-menu .page-link a {
    display: inline-block;
    width: 60px;
    height: 40px;
    padding: 10px 0;
}

.nav-menu-up .page-link span {
    clear: both;
    margin-left: 20px;
}

.nav-menu-up .sub-menu-divide {
    margin: 0 10px;
}

.nav-menu-up .page-menu .common {
    margin-right: 10px;
    background: url(/@images/left_nav_ico.png) 0 -115px no-repeat;
}

.nav-menu-up .page-link:hover .common {
    background: url(/@images/left_nav_ico_hover.png) 0 -115px no-repeat;
}

.nav-menu-up .page-menu .history {
    margin-right: 10px;
    background: url(/@images/left_nav_ico.png) 0 -136px no-repeat;
}

.nav-menu-up .page-link:hover .history {
    background: url(/@images/left_nav_ico_hover.png) 0 -136px no-repeat;
}

.nav-menu-up .shortcut-menu ul li {
    line-height: 50px;
    height: 50px;
}

.nav-menu-up .nav-menu-body {
    height: calc(100% - 210px);
    height: -webkit-calc(100% - 210px);
    height: -moz-calc(100% - 210px);
}

.nav-menu-up .nav-menu-footer {
    padding: 0 10px;
}

.nav-menu-up .admin-info img {
    margin-left: 2px;
}

.nav-menu-up .admin-info {
    margin-bottom: 20px;
}

.nav-menu-up .account-mgt {
    margin-left: 10px;
}

.nav-menu-up .account-mgt .help {
    margin-bottom: 17px;
}
/*header*/

.header .title {
    font-size: 20px;
    float: left;
    cursor: default;
    color: #3d4b66;
    margin-top: 13px;
}

.header .btn-group {
    position: relative;
}

.function-btn {
    float: right;
}

.function-btn li {
    position: relative;
    float: left;
    text-align: center;
}

.function-btn li a {
    display: block;
    overflow: hidden;
    width: 65px;
    height: 55px;
}

.function-btn li a:hover {
    text-decoration: none;
}

.oa-slidebar-taggle {
    display: inline-block;
    width: 12px;
    height: 18px;
    margin: 20px 0 0;
    -webkit-transition: all 0s ease;
    -o-transition: all 0s ease;
    transition: all 0s ease;
    opacity: .5;
    outline: none;
    background: url(/@images/header_ico.png) 0 -80px no-repeat;
}

.oa-slidebar-taggle-open {
    -webkit-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);
}

.function-btn li:hover {
    background: #f6f6f6;
}

.function-btn .notice a {
    width: 65px;
    height: 55px;
}

.function-btn li a:hover .oa-slidebar-taggle {
    opacity: 1;
}

.oa-dropdown-toggle {
    display: inline-block;
    width: 20px;
    height: 4px;
    margin: 28px 0 0 0;
    background: url(/@images/page_head_ico.png) 0 -92px no-repeat;
}

.function-btn li a:hover .oa-dropdown-toggle {
    background: url(/@images/page_head_ico.png) 0 -83px no-repeat;
}

.announcement-notice {
    display: inline-block;
    width: 17px;
    height: 19px;
    margin: 20px 0 0 0;
    text-indent: -9999px;
    background: url(/@images/page_head_ico.png) 0 -60px no-repeat;
}

.announcement-new-tip {
    line-height: 13px;
    position: absolute;
    top: 10px;
    right: 10px;
    height: 16px;
    padding: 2px 5px;
    color: #fff;
    border-radius: 100px;
    background: #fa7252;
}

.function-btn li a:hover .announcement-notice {
  background: url(/@images/page_head_ico.png) 0 -39px no-repeat;
}

.page-topsec-area {
    margin: 13px 25px 0 0;
}
.page-topsec-box {
    /*border-right: 1px solid #eee;*/
}
.page-topsec-box:hover{
    background: transparent !important;
}
.page-topsec-area .topsec-btn {
    display: inline-block;
    width: 18px;
    height: 19px;
    margin: 6px 0 0 0;
    transition-timing-function: ease-in;
    transition-duration: .4s;
    transition-property: background-color;
    background: url(/@images/page_head_ico.png) 0 -20px no-repeat;
}

.function-btn li a:hover .topsec-btn {
    opacity: 1;
}

.page-topsec-area-bg {
    cursor: pointer;
    background: #f6f6f6;
}

.page-topsec-area-bg .page-topsec-inputbox .topsec-btn {
    opacity: 1;
}

.page-topsec-inputbox {
    padding: 0 15px;
    height: 32px;
    border:1px solid #d8e3ef;
    border-radius: 15px;
}
.page-topsec-inputbox.active{
   border:1px solid #adc3d9;
}
.page-topsec-inputbox.active .topsec-btn{
  background: url(/@images/page_head_ico.png) 0 0 no-repeat; 
}
.page-topsec-area .topsec-btn:hover{
  background: url(/@images/page_head_ico.png) 0 0 no-repeat;
}
.page-topsec-inputbox input {
    font-size: 14px;
    line-height: 29px;
    float: left;
    width: 270px;
    height: 29px;
    border: none;
    margin-top: 1px;
    color: #6b7a99;
}

.page-topsec-inputbox input::-moz-placeholder {
    color: #cfd6e5;
}

.page-topsec-inputbox input::-webkit-input-placeholder {
    color: #cfd6e5;
}

.page-topsec-inputbox .topsec-btn {
    float: left;
}

