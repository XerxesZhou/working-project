.aside-novice-guide{
    position: absolute;
    z-index: 999;
    bottom: 52px;
    left: 5px;
    width: 223px;
    padding: 10px 23px 8px 8px;
    color: #555555;
    border-radius: 3px;
    background: #fff;
    cursor: default;
    box-shadow: 0 1px 2px #ddd;
}
.aside-novice-guide a.close{
    width: 20px;
    height: 20px;
    top: 7px;
    right: -3px;
}
.aside-novice-guide a.close i{
  background: url(/static/img/button_pm_close.png) center center no-repeat;
  width: 10px;
  height: 10px;
}
.aside-novice-guide a.close:hover i{
    opacity: .7;
}
.aside-novice-guide-content > span{
    margin-bottom: 8px;
}
.aside-novice-guide-arrow{
    position: absolute;
