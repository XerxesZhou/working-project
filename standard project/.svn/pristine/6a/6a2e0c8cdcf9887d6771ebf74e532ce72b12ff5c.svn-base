.theme-set-content {

}
.theme-set-header span.title{
   font-size: 15px;
   color: #334455;
   display: block;
   margin-bottom: 10px;
} 
.theme-set-header p{
    color: #667b8f;
}
.theme-set-list{
    margin-top: 21px;
    overflow: hidden;
}
.theme-set-list li{
    float: left;
    margin-right: 72px;
    margin-bottom: 25px;
    cursor: pointer;
}
.theme-set-list li:nth-child(3),
.theme-set-list li:nth-child(6){
    margin-right: 0px;
}
.theme-set-list li > div{
    display: inline-block;
    margin: 5px;
    border:1px solid #d8e3ef;
    background: #fff;
    border-radius: 3px;
    padding: 5px 5px 3px 5px;
    clear: both;
}
.theme-set-list li:hover > div{
    border:1px solid #adc3d9;
}
.theme-set-list li > span{
    display: block;
    margin-left: 10px;
}

