@charset 'UTF-8';
/*layout*/

.wrapper {
    width: 100%;
    min-width: 1200px;
    height: 100%;
    min-height: 600px;
    margin: 0;
    padding: 0;
    background: #fff;
}

.content-wrapper {
    display: inline-block;
    width: 100%;
    padding: 15px;
}

.nav-menu {
    font-weight: 100;
    position: fixed;
    z-index: 100;
    top: 0;
    left: 0;
    width: 220px;
    height: 100%;
    color: #fff;
    background: #363d4d;
}

.main-content {
    position: absolute;
    right: 0;
    left: 220px;
    overflow: hidden;
    height: 100%;
    background-color: #e6e9f0;
}

.header {
   /* line-height: 55px;*/
    position: relative;
    top: 0;
    height: 55px;
    padding: 0 0 0 30px;
    background: #fff;
    border-bottom: 1px solid #d8e3ef;
}
/*aside*/

.logo {
    height: 54px;
    background: #3e4659;
}

.logo-img {
    float: left;
    margin: 14px 0 0 20px;
    color: #f2f2f2;
}
.logo-img img{
    height: 26px;
    max-width: 100px;
}
.logo-img span.logo-pic{
    background: url(/@images/theme_default_logo.png) left center no-repeat;
    display: inline-block;
    height: 26px;
    max-width: 120px;
    min-width: 86px;
    background-size: 81px 25px;
}
.industry-logo {
    font-size: 15px;
    display: inline-block;
    margin: 22px 0 0 5px;
    color: #858b95;
}
.sidebar-toggle {
    display: inline-block;
    float: right;
    width: 20px;
    height: 20px;
    margin: 20px 20px 0 0;
    text-align: center;
    color: #fff;
}

.sidebar-toggle span {
    display: inline-block;
    width: 14px;
    height: 14px;
    background: url(/@images/left_nav_ico.png) 0 0px no-repeat;
}

.sidebar-toggle:hover span{
    background: url(/@images/left_nav_ico_hover.png) 0 0px no-repeat;
}

.nav-menu-footer {
    height: 124px;
    padding: 0 20px;
}

.admin-info {
    overflow: hidden;
    padding-top: 20px;
    border-top: 1px solid #47546a;
}
.admin-info a.admin-avatar{
  width: 40px;
  height: 40px;
  background: #fff;
  display: inline-block;
  float: left;
  border-radius: 50px;
  line-height: 40px;
  text-align: center;
  margin-right: 10px;
}
.account-mgt {
    margin: 24px 0 0 0;
}

.account-mgt a {
    display: inline-block;
    float: left;
    width: 20px;
    height: 20px;
}

.account-mgt .setting {
    margin-right: 30px;
    background: url(/@images/left_nav_ico.png) 0 -156px no-repeat;
}

.account-mgt .help {
    background: url(/@images/left_nav_ico.png) 0 -175px no-repeat;
    margin-top: -1px;
}

.account-mgt .setting:hover {
    background: url(/@images/left_nav_ico_hover.png) 0 -156px no-repeat;
}

.account-mgt .help:hover {
    background: url(/@images/left_nav_ico_hover.png) 0 -175px no-repeat;
}

.admin-info .userpic {
  float: left;
  width: 36px;
  height: 36px;
  border-radius: 50px;
  text-align: center;
  vertical-align: middle;
  text-align: center;
  display: block;
  margin: 0 auto;
  margin-top: 2px;
  margin-left: 2px;
}

.nav-menu-footer .personal-info span {
    display: block;
    overflow: hidden;
    max-width: 120px;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.nav-menu-footer .personal-info a:hover {
    text-decoration: none;
}

.nav-menu-footer .personal-info .peopel-name {
    font-size: 13px;
    margin-bottom: 3px;
    color: #a3b1cc;
}

.nav-menu-footer .personal-info .company-name {
    color: #a3b1cc;
}
/*nav-menu-body*/

.nav-menu-body {
    overflow: hidden;
    height: calc(100% - 180px);
    height: -webkit-calc(100% - 180px);
    height: -moz-calc(100% - 180px);
    padding-top: 10px;
}

.sub-menu {
    height: 100%;
}

.page-menu {
    margin: 0 0 20px 0;
}

.page-link {
    padding: 0px 15px 0px 20px;
    cursor: default;
    height: 40px;
    line-height: 40px;
}

.page-link a {
    font-size: 15px;
    color: #a3b1cc !important;
}

.page-link a:hover {
    text-decoration: none;
}

.page-link span {
    display: inline-block;
    float: left;
    width: 18px;
    height: 18px;
}

.page-link .home {
    margin-top: 10px;
    margin-right: 10px;
    background: url(/@images/left_nav_ico.png) 0 -55px no-repeat;
}

.page-link .crm {
    margin-top: 10px;
    margin-right: 10px;
    background: url(/@images/left_nav_ico.png) 0 -76px no-repeat;
}

.page-link .count {
    margin-top: 11px;
    margin-right: 10px;
    background: url(/@images/left_nav_ico.png) 0 -96px no-repeat;
}

.crm-page-toolbar {
    float: right;
}

.page-menu li {
    cursor: default;
}
.page-menu > ul {
    margin-bottom: 10px;
}
.crm-page-toolbar span.menu-setting-span {
    margin-right: 10px;
}

.crm-page-toolbar .menu-setting {
    display: inline-block;
    float: left;
    width: 14px;
    height: 14px;
    margin: 14px 15px 0 0;
    background: url(/@images/left_nav_ico.png) 0 -42px no-repeat;
}

.crm-page-toolbar .menu-setting:hover {
    background: url(/@images/left_nav_ico_hover.png) 0 -42px no-repeat;
}

.crm-page-toolbar .menu-chevron-down {
    display: inline-block;
    float: left;
    width: 11px;
    height: 11px;
    margin: 16px 0 0 0;
    background: url(/@images/left_nav_ico.png) 0 -32px no-repeat;
}
/*.crm-page-toolbar .menu-chevron-down:hover{
    background: url(/@images/left_nav_ico_hover.png) 0 -32px no-repeat;
}*/
.crm-page-toolbar .menu-chevron-up {
    /* -webkit-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);*/
}

.sub-menu-divide {
    display: block;
    height: 1px;
    margin: 0 20px;
    background: #475469;
}
.page-link.selected {
    background: #3f4a60;
    box-shadow:inset 2px 0px 0px #00aaef; 
}
.page-link.selected .home{
  background: url(/@images/left_nav_ico_hover.png) 0 -55px no-repeat;
}
.page-link.selected a{
  color: #00aaef !important;
}
.page-link.active{
    background: #3f4a60;
}
.page-link:hover {
    background: #3f4a60;
}

.page-link.active a {
    color: #fff !important;
}
.page-link:hover .home {
    background: url(/@images/left_nav_ico_hover.png) 0 -55px no-repeat;
}
.page-link:hover .crm {
    background: url(/@images/left_nav_ico_hover.png) 0 -76px no-repeat;
}
.page-link:hover .count {
    background: url(/@images/left_nav_ico_hover.png) 0 -96px no-repeat;
}
.page-link.selected .home {
    background: url(/@images/left_nav_ico_hover.png) 0 -55px no-repeat;
}
.page-link.selected .crm {
    background: url(/@images/left_nav_ico_hover.png) 0 -76px no-repeat;
}
.page-link.selected .count {
    background: url(/@images/left_nav_ico_hover.png) 0 -96px no-repeat;
}

.page-link:hover a {
    color: #fff !important;
}
/*nav-menu-crm*/

.nav-menu-crm {
    display: none;
    overflow: hidden;
    max-height: 275px;
    margin: 5px 15px 5px 0;
}

.nav-menu-crm li {
    line-height: 30px;
    height: 37px;
    padding: 0 0 0 50px;
    cursor: default;
}

.nav-menu-crm li a {
    font-size: 13px;
    display: block;
    overflow: hidden;
    max-width: 127px;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #d6d8dd;
}

.nav-menu-crm li a:hover {
    text-decoration: none;
    color: #ebbb42;
}

.nav-menu-crm li.active a {
    text-decoration: none;
    color: #ebbb42;
}
/*shortcut-menu*/

.shortcut-menu {
    overflow: hidden;
    height: -webkit-calc(100% - 187px);
    height: -moz-calc(100% - 187px);
    height: calc(100% - 187px);
    margin: 25px 0 0 0;
}

.shortcut-menu h3 {
    font-size: 14px;
    font-weight: normal;
    margin: 0 20px 5px;
    color: #a3b1cc;
}

.shortcut-menu ul li {
    line-height: 33px;
    height: 33px;
}

.shortcut-menu ul li a {
    display: block;
    overflow: hidden;
    max-width: 200px;
    padding: 0 20px 0 25px;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #a3b1cc;
}
.shortcut-menu ul li a:hover{
    color: #fff;
}
.menu-common-use {
    margin-bottom: 15px;
}

.menu-common-use a span {
    display: inline-block;
    float: left;
    width: 14px;
    height: 14px;
    margin-top: 7px;
    margin-right: 6px;
    background: url(/@images/left_nav_ico.png) 0 -195px no-repeat;
}
/*.menu-item-history.menu-common-use a span{
    background: none;
}*/

.menu-common-use a:hover {
    text-decoration: none;
}

.menu-common-use li:hover {
    background: #3f4a61;
}
.menu-common-use li:hover a{
    color: #fff;
}
.menu-common-use li:hover span {
    float: left;
    height: 14px;
    margin-top: 7px;
    background: url(/@images/left_nav_ico_hover.png) 0 -195px no-repeat;
}

.nav-menu-close {
    display: none;
}

.menu-common-use li.active {
    background: #303e4d;
}

.menu-common-use li.active a span {
    height: 15px;
    background: url(/@images/left_nav_ico_hover.png) 0 -195px no-repeat;
}

.more-page-menu {
    line-height: 28px;
    position: absolute;
    bottom: 126px;
    display: inline-block;
    width: 178px;
    background: #ccaa66;
    height: 30px;
    margin-left: 20px;
    text-align: center;
    color: #fff;
    border-radius: 3px;
    border:1px solid #ccaa66;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}

.more-page-menu span {
    display: inline-block;
    width: auto;
    padding-right: 20px;
    text-align: center;
    color: #fff;
    background: url(/@images/crm/more_drop_down.png) right 9px no-repeat;
}
.more-page-menu:hover{
    background: #e6b85c;
}
.more-page-menu:hover span{
    opacity: 1;
}
/*menu-item-history*/

.menu-item-history li.active {
    background: #3f4a61;
}

.menu-item-history li:hover {
    background: #3f4a61;
}

.menu-item-history li a:hover {
    text-decoration: none;
}
