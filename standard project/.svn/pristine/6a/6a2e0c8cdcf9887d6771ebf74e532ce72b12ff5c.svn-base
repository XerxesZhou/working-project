    position: absolute;
    z-index: 15;
    bottom: 0;
    width: 100%;
    height: 40px;
    padding: 0 20px;
    color: #c59400;
    background: #ffe08d;
}

.maintenance_bar span.msg {
    display: inline-block;
    padding: 2px 24px;
    background: url(/@images/maintenance_tip.png) left 14px no-repeat;
}

.maintenance_bar .maintenance-close {
    position: absolute;
    right: 10px;
    top:0px;
    display: inline-block;
    width: 10px;
    height: 10px;
    margin-top: 16px;
    opacity: .5;
    background: url(/@images/maintenance.png) 0 0 no-repeat;
}

.maintenance_bar .maintenance-close:hover {
    opacity: 1;
}
/* pagecount */

.rk-paging .mxx-pagination-container {
    margin: 15px 0 15px 0;
    text-align: center;
}

.rk-paging .mxx-pagination-container span {
    display: inline-block;
    overflow: hidden;
}

.rk-paging .mxx-pagination-pages span {
    line-height: 20px;
    display: inline-block;
    float: left;
    min-width: 14px;
    height: 22px;
    margin: 2px 5px;
    padding: 0 7px;
    cursor: pointer;
    text-align: center;
    border: 1px solid #d9d9d9;
    border-radius: 3px;
    background: #fff;
}

.rk-paging .mxx-pagination-pages span:hover {
    color: #fff;
    border: 1px solid #a8b5c4;
    background: #a8b5c4;
}

.rk-paging .mxx-pagination-prev {
    padding: 0 10px !important;
    text-indent: -9999px;
    background: url(/@images/pagination_prev.png) center center no-repeat !important;
}

.rk-paging .mxx-pagination-next {
    padding: 0 10px !important;
    text-indent: -9999px;
    background: url(/@images/pagination_next.png) center center no-repeat !important;
}

.rk-paging .mxx-pagination-prev:hover {
    background: #a8b5c4 url(/@images/pagination_prev_hover.png) center center no-repeat !important;
}

.rk-paging .mxx-pagination-next:hover {
    background: #a8b5c4 url(/@images/pagination_next_hover.png) center center no-repeat !important;
}

.rk-paging .mxx-pagination-summary {
    font-size: 14px;
    padding: 2px 4px;
    padding: 0 10px;
}

.rk-paging .mxx-pagination-jump {
    font-size: 14px;
}

.rk-paging .mxx-pagination-jump input {
    line-height: 21px;
    width: 30px;
    height: 21px;
    margin-left: 8px;
    padding-left: 3px;
}

.rk-paging .mxx-pagination-sizelist {
    overflow: hidden;
    width: 24px;
    height: 14px;
    padding: 2px;
}

.rk-paging .mxx-pagination-container .curr {
    cursor: default;
    color: #fff !important;
    border: 1px solid #a8b5c4;
    background: #a8b5c4;
}

.rk-paging span.mxx-pagination-summary-total,
.rk-paging span.mxx-pagination-summary-start,
.rk-paging span.mxx-pagination-summary-line,
.rk-paging span.mxx-pagination-summary-end,
.rk-paging span.mxx-pagination-summary-splitter {
    margin: 0;
    padding: 0;
    cursor: default;
    border: none;
}

.rk-paging span.mxx-pagination-summary-total:hover {
    color: #343d47;
    border: none !important;
}

.rk-paging span.mxx-pagination-summary-start:hover,
.rk-paging span.mxx-pagination-summary-line:hover,
.rk-paging span.mxx-pagination-summary-end:hover,
.rk-paging span.mxx-pagination-summary-splitter:hover {
    color: #343d47;
    border: none !important;
}

