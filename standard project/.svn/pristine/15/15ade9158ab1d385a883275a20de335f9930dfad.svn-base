/**导航栏 start**/
.menu_list{width:100%;margin:0 auto; float:left; z-index:999; }
.menu_head{
	        height: 40px;
            line-height: 40px;
            padding-left: 40px;
            font-size: 16px;
            cursor: pointer;
            display:block;
            border-bottom: 1px solid #FFF;
           /* position: relative;*/
            margin: 0px;
            font-weight: bold;
            color: #FFF;
}
.menu_body{
	line-height: 38px;
	border-left: 1px solid #e1e1e1;
	border-right: 1px solid #e1e1e1;
}
.menu_body a
{            letter-spacing:1px;   
             display:block;
             height:35px;
             line-height:35px;
             padding-left:62px;
             color:#777777;
             background:#fff;
             text-decoration:none;
             border-bottom:1px solid #e1e1e1;
             font-size:1vw;
  }
.menu_body a:hover{text-decoration:none;}


.top_list{   margin:0px 0px 0px 200px;
             height:50px;
             width:auto;
            background-color:White;
            z-index:999; 
            /*position:fixed;*/
            border-bottom:1px solid #000;
           }   
           
           
.top_list ul{ width:100%; 
 height:100%;
  border-bottom:1px solid #e0e0e0;
  margin:0px 0px 0px 0px;
}        
               
                   
.top_list ul li 
{
list-style-type:none; 
float:left;
height:100%;
line-height: 50px;
margin-right: 40px;
}
                 
.top_list ul li .li_list
{
    font-size:16px;
   `height:69%;
   *height:100% !improtant;
    padding: 0px 5px 0px 5px;
    color:#108BE5; /*  文字颜色 */
    display:inline-block; /* 此元素将显示为块级元素，此元素前后会带有换行符 */
 /* 行高    line-height: 2.5em; */
  /*  padding:20px 30px 0px 30px;  内部填充的距离 */
    text-decoration: none; /* 不显示超链接下划线 */
    white-space: nowrap; /* 对于文本内的空白处，不会换行，文本会在在同一行上继续，直到遇到 <br> 标签为止。 */
    }                 

/* 所有class为menu的div中的ul中的a样式(鼠标移动到元素中的样式) */
.top_list ul li .li_list:hover
{ 

  height:41px;
  line-height:41px;
  color:White;
  background-color: #D2CCCC; /* 背景色 */
    /* color:#108BE5;  文字颜色 */
    text-decoration: none; /* 不显示超链接下划线 */
}
/* 所有class为menu的div中的ul中的a样式(鼠标点击元素时的样式) */
.top_list ul li .li_list:active
{
    background-color: #ccc; /* 背景色 */
    color:#108BE5;  /* 文字颜色 */
    text-decoration: none; /* 不显示超链接下划线 */
}


                
