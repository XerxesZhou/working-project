namespace SmartSoft.Domain.Common
{
	using System;
	
	/// <summary>
	/// sysStream 
	/// </summary>
	[Serializable]
	public class sysStream
	{
		public sysStream()
		{
			
		}
		
		private string _tablename;
		private int _maxid;
		
		///<sumary>
		/// 系统表名
		///</sumary>
		public string TableName
		{
			get{return _tablename;}
			set{_tablename = value;}
		}
		///<sumary>
		/// 流水号
		///</sumary>
		public int MaxID
		{
			get{return _maxid;}
			set{_maxid = value;}
		}
	}
}
