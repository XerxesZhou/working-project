namespace SmartSoft.Domain.Data
{
    using System;

    /// <summary>
    /// CustomerFeedback 
    /// </summary>
    [Serializable]
    public class CustomerFeedback
    {
        public CustomerFeedback()
        {

        }

        private int _cfbid;
        private int _cfbcustomerid;
        private int _cfbfeedbacktypeid;
        private string _cfborderrelated;
        private string _cfbcontent;
        private string _cfblinkman;
        private string _cfbtelephone;
        private string _cfbemail;
        private int _cfboperatorid;
        private int _cfbaddoperatorid;
        private DateTime _cfbadddate;
        private int? _cfbmodifyoperatorid;
        private DateTime? _cfbmodifydate;
        private int _cfbstatusid;
        private string _cfbresult;
        private int? _cfbhandleoperatorid;
        private DateTime? _cfbhandledate;

        ///<sumary>
        /// 客户反馈ID
        ///</sumary>
        public int cfbID
        {
            get { return _cfbid; }
            set { _cfbid = value; }
        }
        ///<sumary>
        /// 客户
        ///</sumary>
        public int cfbCustomerID
        {
            get { return _cfbcustomerid; }
            set { _cfbcustomerid = value; }
        }
        ///<sumary>
        ///  反馈类型：投诉  问题  建议  表扬 
        ///</sumary>
        public int cfbFeedbackTypeID
        {
            get { return _cfbfeedbacktypeid; }
            set { _cfbfeedbacktypeid = value; }
        }
        ///<sumary>
        /// 相关合同
        ///</sumary>
        public string cfbOrderRelated
        {
            get { return _cfborderrelated; }
            set { _cfborderrelated = value; }
        }
        ///<sumary>
        /// 反馈内容
        ///</sumary>
        public string cfbContent
        {
            get { return _cfbcontent; }
            set { _cfbcontent = value; }
        }
        ///<sumary>
        /// 联系人
        ///</sumary>
        public string cfbLinkman
        {
            get { return _cfblinkman; }
            set { _cfblinkman = value; }
        }
        ///<sumary>
        /// 联系电话
        ///</sumary>
        public string cfbTelephone
        {
            get { return _cfbtelephone; }
            set { _cfbtelephone = value; }
        }
        ///<sumary>
        /// 联系电邮
        ///</sumary>
        public string cfbEmail
        {
            get { return _cfbemail; }
            set { _cfbemail = value; }
        }
        ///<sumary>
        /// 业务员
        ///</sumary>
        public int cfbOperatorID
        {
            get { return _cfboperatorid; }
            set { _cfboperatorid = value; }
        }
        ///<sumary>
        /// 录入人
        ///</sumary>
        public int cfbAddOperatorID
        {
            get { return _cfbaddoperatorid; }
            set { _cfbaddoperatorid = value; }
        }
        ///<sumary>
        /// 录入时间
        ///</sumary>
        public DateTime cfbAddDate
        {
            get { return _cfbadddate; }
            set { _cfbadddate = value; }
        }
        ///<sumary>
        /// 修改人 
        ///</sumary>
        public int? cfbModifyOperatorID
        {
            get { return _cfbmodifyoperatorid; }
            set { _cfbmodifyoperatorid = value; }
        }
        ///<sumary>
        /// 修改时间
        ///</sumary>
        public DateTime? cfbModifyDate
        {
            get { return _cfbmodifydate; }
            set { _cfbmodifydate = value; }
        }
        ///<sumary>
        /// 反馈单状态：未处理,正在处理,处理完毕
        ///</sumary>
        public int cfbStatusID
        {
            get { return _cfbstatusid; }
            set { _cfbstatusid = value; }
        }
        ///<sumary>
        /// 处理结果
        ///</sumary>
        public string cfbResult
        {
            get { return _cfbresult; }
            set { _cfbresult = value; }
        }
        ///<sumary>
        /// 申请处理人
        ///</sumary>
        public int? cfbHandleOperatorID
        {
            get { return _cfbhandleoperatorid; }
            set { _cfbhandleoperatorid = value; }
        }
        ///<sumary>
        /// 处理日期
        ///</sumary>
        public DateTime? cfbHandleDate
        {
            get { return _cfbhandledate; }
            set { _cfbhandledate = value; }
        }
    }
}
