body
{
    
}

.ViewAll
{
    width:98%;
    margin:0 auto;
}

.Page
{
   
}

.HeadTitle
{
    font-size: 24px;
    color: #fff;
    background-color: #459df5;
    margin: 25px 0;
    padding: 0px 0px 6px 10px;
}

.Content
{
    padding-left:50px;
}

p
{
    line-height:30px;
}

img
{
    max-width:100%;
}

#Main
{
    overflow:auto;
}

#Main .MenuBody a:hover
{
    background-color: #eee;
}
#Main h3:hover
{
    background-color: #09CFF9;
}

.MenuHead
{
    height: 40px;
    line-height: 40px;
    padding-left: 40px;
    font-size: 16px;
    background-color: #0082E5;
    cursor: pointer;
    border-bottom: 1px solid #FFF;
    position: relative;
    margin: 0px;
    font-weight: bold;
    background: #0082E5 url(img/pro_left.png) center right no-repeat;
    color: #FFF;
}

.into
{
    color: Red !important;
}

.intoH3
{
    background-color: #09CFF9 !important;
}

.MenuBody
{
    line-height: 38px;
    border-left: 1px solid #e1e1e1;
    border-right: 1px solid #e1e1e1;
    display:none;
}

.MenuBody a
{
    letter-spacing: 1px;
    display: block;
    height: 35px;
    line-height: 35px;
    padding-left: 62px;
    color: #777777;
    background: #fff;
    text-decoration: none;
    border-bottom: 1px solid #e1e1e1;
    font-size: 1vw;
    cursor:pointer;
}

.helpLeft
{
    float:left;
    width:18%;
}

.helpRight
{
    float:left;
    width:82%;
}

.rightTitle
{
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-size: 30px;
    text-shadow: none;
}

