.black_overlay
{
    position: fixed;
    z-index: 1000;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    filter: alpha(opacity=80);
    opacity: 0.3;
    overflow: hidden;
    background-color: #000;
}

.CssForTrans
{
    z-index: 99999; 
    border-radius:5px; 
    margin:auto; 
    position: fixed; 
    width: 35%; 
    background-color: #fff; 
    height:20%; 
    display:none; 
    top:0; 
    left:0; 
    right:0; 
    bottom:0;
}
