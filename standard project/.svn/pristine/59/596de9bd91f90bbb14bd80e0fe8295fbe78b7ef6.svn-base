    text-shadow: none;
    font-weight: 400;
}

.picView
{
    text-align: center;
    max-width: 30%;
    margin-right: 10px;
}
/*点赞评论*/
.cssLike
{
    color:#3C96DE;
}
.cssNotLike
{
    color:Gray;
}
/*点赞评论结束*/

