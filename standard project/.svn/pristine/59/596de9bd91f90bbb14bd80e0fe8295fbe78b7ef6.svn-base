/*菜单滑动*/
#divNavigation
{
    width: 100%;
    height: 40px;
    overflow: hidden;
    position: fixed;
    margin: 0;
    top: 80px;
    background-color: white;
    z-index: 1003;
}
        
#divNavigation ul
{
    position: absolute;
    height: 40px;
}
        
#divNavigation ul li
{
    float: left;
    width: 33.33%;
    height: 40px;
}
        
#divNavigation ul a
{
    width: 100%;
    color: #3C96DE;
    font-weight: normal;
    line-height: 40px;
    display: block;
    height: 40px;
    text-align: center;
    cursor:pointer;
}
        
#divNavigation .preNext
{
    width: 33px;
    height: 100px;
    position: absolute;
    top: -33px;
    background: url(../images/sprite.png) no-repeat 0 0;
    cursor: pointer;
}
#divNavigation .pre
{
    left: 0;
}
#divNavigation .next
{
    right: 0;
    background-position: right top;
}
/*菜单滑动结束*/
.li_title
{
    line-height: 40px;
    height: 40px;
    text-align: center;
    background-color: #3C96DE;
    color: #fff !important;
