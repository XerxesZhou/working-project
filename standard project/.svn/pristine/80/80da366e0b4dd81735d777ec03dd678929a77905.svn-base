namespace SmartSoft.Domain.Data
{
    using System;

    /// <summary>
    /// defCommon
    /// </summary>
    [Serializable]
    public class defCommon
    {
        public defCommon()
        {

        }

        private int _id;
        private string _name;
        private string _tablename;
        private string _remark;
        private bool _usetag;

        ///<sumary>
        /// ID
        ///</sumary>
        public int ID
        {
            get { return _id; }
            set { _id = value; }
        }
        ///<sumary>
        /// 名称
        ///</sumary>
        public string Name
        {
            get { return _name; }
            set { _name = value; }
        }
        ///<sumary>
        /// 类型编号
        ///</sumary>
        public string TableName
        {
            get { return _tablename; }
            set { _tablename = value; }
        }
        ///<sumary>
        /// 备注
        ///</sumary>
        public string Remark
        {
            get { return _remark; }
            set { _remark = value; }
        }
        ///<sumary>
        /// 已用标志
        ///</sumary>
        public bool UseTag
        {
            get { return _usetag; }
            set { _usetag = value; }
        }

        public int OrderBy
        {
            get;
            set;
        }
    }
}
